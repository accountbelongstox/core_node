# PUT_IP 脚本
vi /etc/share_put_ip.sh

#!/bin/bash
NET_INTERFACE="pppoe-wan"
for i in {1..3}; do
  IP=$(ip addr show $NET_INTERFACE | grep 'inet ' | awk '{print $2}' | cut -d/ -f1)
  if [ -z "$IP" ]; then
    echo "None IP"
    exit 1
  fi
  curl "https://ip.hk.12gm.com?put_ip=$IP"
  echo "$IP"
  sleep 20
done

#计划任务
* * * * * /etc/share_put_ip.sh
 service cron reload

