# 应用名称，用于确定要加载哪个应用
APP_NAME=bing_dict

# 服务器端口
PORT=3000

# 环境（development, production, test）
NODE_ENV=development

# 数据库连接字符串（如果需要的话）
DATABASE_URL=mongodb://localhost:27017/bing_dict

# API密钥（如果您的应用需要外部服务）
API_KEY=your_api_key_here

# 日志级别
LOG_LEVEL=info

# 最大并发浏览器实例数
MAX_BROWSER_INSTANCES=2

# Chrome可执行文件路径（如果需要指定）
CHROME_PATH=/usr/bin/google-chrome

# 下载路径
DOWNLOAD_PATH=/path/to/downloads

# 是否启用无头模式（true/false）
HEADLESS=false

# 默认超时时间（毫秒）
DEFAULT_TIMEOUT=30000